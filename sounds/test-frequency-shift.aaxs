<?xml version="1.0"?>

<aeonwave>
 <sound frequency="220">
  <waveform src="sine"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">10.0</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
 </emitter>

 <audioframe>
  <effect type="frequency-shift" src="sine">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">2.0</param>
    <param n="2">110.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </audioframe>

</aeonwave>
