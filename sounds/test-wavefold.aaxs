<?xml version="1.0"?>

<aeonwave>
 <sound frequency="220">
  <waveform src="sine"/>
 </sound>

 <emitter looping="true">
  <effect type="wavefold" src="sine">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">0.5</param>
    <param n="2">0.6</param>
    <param n="3">0.2</param>
   </slot>
  </effect>
  <filter type="timed-gain">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">10.0</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
