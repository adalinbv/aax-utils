   <slot n="0">
    <param n="0">0.5</param>
    <param n="1">0.3</param>
    <param n="2">1.5</param>
    <param n="3">0.3</param>
   </slot>
   <slot n="1">
    <param n="0">0.5</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.0</param>
   </slot>

  </effect>
 </emitter>

</aeonwave>
