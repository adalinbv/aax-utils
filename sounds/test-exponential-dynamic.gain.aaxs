<?xml version="1.0"?>

<aeonwave>
 <sound frequency="440">
  <waveform src="sine"/>
  <waveform src="sine" processing="modulate" pitch="1.0"/>
 </sound>

 <emitter looping="true">
  <filter type="dynamic-gain" src="sawtooth|exponential">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">3.0</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
  <effect type="dynamic-pitch" src="sawtooth|exponential">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">3.0</param>
    <param n="2">2.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>

 </emitter>

</aeonwave>
